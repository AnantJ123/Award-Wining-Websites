1. Constructor function
// jab aapke paas aisa koi bhi mauka ho ke apke ek
//  jaisi property waale bahut saare elements baanae hai us waqt aap constructor fnc use kar sakte ho.

function circularButtonBanao(color){
    this.radius = 2;
    this.color=color;
    this.icon=flase;
    this.pressable=true;
} 

var redbtn = new circularButtonBanao("red");
var greenbtn = new circularButtonBanao("green");

